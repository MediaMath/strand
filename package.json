{
  "name": "strand",
  "version": "1.1.3",
  "description": "MediaMath UI-Framework based on polymer.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/MediaMath/strand.git"
  },
  "scripts": {
    "mm2strand": "(cd srcOriginal || cp -r src srcOriginal) && npm install replace renamer && replace 'mm-' 'strand-' grunt src -r && renamer --find 'mm-' --replace 'strand-' src/**/* src/**",
    "noconflict": "npm install replace && replace 'polymer/polymer.html' 'polymerLegacy/polymer.html' src -r && grunt clean build:dist copy:dist",
    "bower": "bower",
    "grunt": "grunt"
  },
  "contributors": [
    "Anthony Koerber <akoerber@mediamath.com>",
    "Dan Lasky <dlasky@mediamath.com>",
    "Justin Moore <jmoore@mediamath.com>",
    "Derek Olson <dolson@mediamath.com>"
  ],
  "license": "BSD",
  "devDependencies": {
    "grunt": "^0.4.0",
    "grunt-banner": "^0.3.1",
    "grunt-bump": "^0.3.0",
    "grunt-contrib-clean": "*",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "*",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "*",
    "grunt-css-url-embed": "*",
    "grunt-gh-pages": "^0.10.0",
    "grunt-github-changes": "0.0.6",
    "grunt-hogan-static": "*",
    "grunt-mocha-phantomjs": "*",
    "grunt-sass": "*",
    "grunt-shell": "^1.1.2",
    "grunt-text-replace": "^0.4.0",
    "grunt-vulcanize": "0.5.0",
    "load-grunt-tasks": "*"
  },
  "dependencies": {
    "grunt-contrib-jshint": "^0.10.0",
    "marked": "^0.3.3",
    "mocha": "^1.21.4"
  }
}
